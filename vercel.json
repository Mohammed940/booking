{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/bot"
    },
    {
      "src": "/api/bot",
      "dest": "/api/bot.js"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "/api/bot.js"
    },
    {
      "src": "/api/reminders/(.*)",
      "dest": "/api/bot.js"
    },
    {
      "src": "/health",
      "dest": "/api/bot.js"
    }
  ]
}